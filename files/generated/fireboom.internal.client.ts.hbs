// @ts-nocheck
import type { OperationArgsWithInput, InternalClient as BaseClient  } from "fireboom-wundersdk/server";
import { {{> operation_partial operations=operations includeInternal=false includeResponseData=false}} } from "./models"

export interface Queries  {
{{#with (filterOperations operations 'isQuery,isNotInternal' true)}}
  {{#each this}}
    "{{path}}": ({{#if hasInput}}options: OperationArgsWithInput<Internal{{name}}Input>{{/if}}) => Promise<{{name}}Response>;
  {{/each}}
{{else}}
  queries: {};
{{/with}}
}

export interface Mutations  {
{{#with (filterOperations operations 'isMutation,isNotInternal' true)}}
  {{#each this}}
    "{{path}}": ({{#if hasInput}}options: OperationArgsWithInput<{{name}}Input>{{/if}}) => Promise<{{name}}Response>;
  {{/each}}
{{else}}
  mutations: {};
{{/with}}
}

export interface InternalClient extends BaseClient<Queries, Mutations> {}