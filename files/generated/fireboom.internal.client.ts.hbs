// @ts-nocheck
import type { OperationArgsWithInput, InternalClient as BaseClient  } from "fireboom-wundersdk/server";
import { {{ modelImports }} } from "./models"

export interface Queries  {
{{#if hasInternalQueries}}
  {{#each internalQueries}}
    "{{path}}": ({{#if hasInternalInput}}options: OperationArgsWithInput<Internal{{operationName}}Input>{{/if}}) => Promise<{{operationName}}Response>;
  {{/each}}
{{else}}
  queries: {};
{{/if}}
}
		
export interface Mutations  {
{{#if hasInternalMutations}}
  {{#each internalMutations}}
    "{{path}}": ({{#if hasInternalInput}}options: OperationArgsWithInput<{{operationName}}Input>{{/if}}) => Promise<{{operationName}}Response>;
  {{/each}}
{{else}}
  mutations: {};
{{/if}}
}

export interface InternalClient extends BaseClient<Queries, Mutations> {}