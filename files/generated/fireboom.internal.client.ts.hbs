import type { OperationArgsWithInput, InternalClient as BaseClient  } from 'fireboom-wundersdk/server'
import { {{> operation_partial operations=operations includeInternal=true includeResponseData=false}} } from './models'

export interface Queries {
{{#each operations}}
    {{#if isQuery}}
    '{{name}}': ({{#if hasInternalInput}}options: OperationArgsWithInput<{{name}}InternalInput>{{/if}}) => Promise<{{name}}Response>;
    {{/if}}
{{/each}}
}

export interface Mutations {
{{#each operations}}
    {{#if isMutation}}
     '{{name}}': ({{#if hasInternalInput}}options: OperationArgsWithInput<{{name}}InternalInput>{{/if}}) => Promise<{{name}}Response>;
    {{/if}}
{{/each}}
}

export interface InternalClient extends BaseClient<Queries, Mutations> {}