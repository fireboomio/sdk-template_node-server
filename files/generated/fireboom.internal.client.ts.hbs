import type { OperationArgsWithInput, InternalClient as BaseClient  } from 'fireboom-wundersdk/server'
import { {{> operation_partial operations=operations includeInternal=true includeInject=false includeResponseData=false}} } from './models'

export interface Queries {
{{#each (filterOperations operations 'isQuery' true)}}
  '{{name}}': ({{#if hasInternalInput}}options: OperationArgsWithInput<Internal{{name}}Input>{{/if}}) => Promise<{{name}}Response>;
{{/each}}
}

export interface Mutations {
{{#each (filterOperations operations 'isMutation' true)}}
  '{{name}}': ({{#if hasInternalInput}}options: OperationArgsWithInput<Internal{{name}}Input>{{/if}}) => Promise<{{name}}Response>;
{{/each}}
}

export interface InternalClient extends BaseClient<Queries, Mutations> {}